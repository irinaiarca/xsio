html
	min-height 100%
body
	background: url("http://i1321.photobucket.com/albums/u553/Sabin_Marcu/irina_pat_zpsb3d439d0.png") #6ab791;
	font-family "Open Sans", Helvetica, Arial, sans-serif
	section
		position relative
	&:not([id=index])
		section
			padding 50px 50px 0 
	a, a:active, a:link, a:visited 
		color inherit
		text-decoration none
	.fancyfont
		font-family Just Me Again Down Here
	a.button
		display inline-block
		text-align center
		height 40px
		line-height 60px
		text-decoration none
		width 200px
		height 60px
		margin 50px 40px 20px 50px
		background linear-gradient(bottom, #8B4513, #cda000 300%) no-repeat
		text-shadow: 0 1px 1px rgba(256, 256, 256, 0.5);
		box-shadow: 0 0px 15px rgba(0, 0, 0, 0.6);
		color black
		border-radius 10px
		font-size 25pt
		&#Story	
			line-height 280px
			display inline-block
			font-size 80pt
			display block
			width 400px
			height 300px
			margin 50px 0 0 50px
		&#about
			margin 100px 0 0 50px
	form
		input 
			width 200px
		position absolute
		top 50%
		
		left 50%
		width 200px
		height 100px
		margin -50px 0 0 -100px
	#admin
		position fixed
		right 0
		top 0
		span
			display inline-block
			margin-right 5px
			border-right solid 1px rgba(0, 0, 0, 0.5)
			padding-right 15px
	#admin a, a#back
		margin -5px 10px 0 0
		padding 0 15px
		width auto
		height auto
		font-size 18pt
		line-height auto
		background none
	a#back
		position fixed
		top 5px
		left 10px
	p
		text-indent 50px
	ul
		margin-left 100px

	table
		position fixed
		top 50%
		left 50%
		margin : -150px 0 0 -150px

	#logintext
		font-family Just Me Again Down Here
		color #8B4513
		font-size 100pt
		text-align center
		margin 80px 0 0 0
	#logo
		display inline-block
		display block
		margin -310px 0 0 600px
		height 320px
		width 470px
	#help
		margin -112px 0 0 0
	.choiceicon
		margin -150px 0 0 50px
		&#pipp
			margin-left -175px
		position fixed
		left 50%
		top 50% 
	#tori
		display block
		margin 0 auto
	
	#startjoc
		display inline-block
		margin -50px 0 0 500px
	#fin
		display block
		margin -100px auto
	#finbutton
		display inline-block
		margin 0 0 0 520px
	#rtard
		text-align center
	#isto
		display inline-block
		margin 0 170px 0 170px
	#didukno
		display block
		margin 0 auto
html 
	@import "game"